{"ast":null,"code":"// import HelloWorld from './components/HelloWorld.vue'\nexport default {\n  // name: 'App',\n  // components: {\n  //   HelloWorld\n  // }\n  data() {\n    return {\n      counter: 0,\n      name: '',\n      lastName: '' // fullName:'',\n\n    };\n  },\n\n  methods: {\n    add(num) {\n      this.counter += num;\n    },\n\n    remove(num) {\n      this.counter -= num;\n    },\n\n    setName() {\n      this.name = event.target.value;\n    },\n\n    resetName() {\n      console.log('reset name executed');\n      this.name = '';\n      this.lastName = '';\n    }\n\n  },\n  watch: {\n    name(value) {\n      if (value === '') {\n        return '';\n      } else {\n        return value;\n      }\n    }\n\n  },\n  computed: {\n    fullName() {\n      console.log('full name executed');\n\n      if (this.name === '') {\n        return '';\n      } else {\n        return this.name + ' ' + 'Shayo';\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAgBA;AAEA,eAAe;AACb;AACA;AACA;AACA;AACAA,MAAI,GAAE;AACJ,WAAO;AACLC,aAAO,EAAC,CADH;AAELC,UAAI,EAAC,EAFA;AAGLC,cAAQ,EAAC,EAHJ,CAIL;;AAJK,KAAP;AAMD,GAZY;;AAabC,SAAO,EAAC;AACNC,OAAG,CAACC,GAAD,EAAK;AACN,WAAKL,OAAL,IAAgBK,GAAhB;AACD,KAHK;;AAINC,UAAM,CAACD,GAAD,EAAK;AACT,WAAKL,OAAL,IAAgBK,GAAhB;AACD,KANK;;AAONE,WAAO,GAAE;AACP,WAAKN,IAAL,GAAYO,KAAK,CAACC,MAAN,CAAaC,KAAzB;AACD,KATK;;AAUNC,aAAS,GAAE;AACTC,aAAO,CAACC,GAAR,CAAY,qBAAZ;AACA,WAAKZ,IAAL,GAAY,EAAZ;AACA,WAAKC,QAAL,GAAgB,EAAhB;AACD;;AAdK,GAbK;AA6BbY,OAAK,EAAC;AACJb,QAAI,CAACS,KAAD,EAAO;AACT,UAAGA,KAAI,KAAM,EAAb,EAAgB;AACd,eAAO,EAAP;AACD,OAFD,MAEK;AACH,eAAOA,KAAP;AACF;AACF;;AAPI,GA7BO;AAsCbK,UAAQ,EAAC;AACPC,YAAQ,GAAE;AACRJ,aAAO,CAACC,GAAR,CAAY,oBAAZ;;AACA,UAAG,KAAKZ,IAAL,KAAc,EAAjB,EAAqB;AACnB,eAAO,EAAP;AACD,OAFD,MAEK;AACH,eAAO,KAAKA,IAAL,GAAY,GAAZ,GAAkB,OAAzB;AACF;AACF;;AARO;AAtCI,CAAf","names":["data","counter","name","lastName","methods","add","num","remove","setName","event","target","value","resetName","console","log","watch","computed","fullName"],"sourceRoot":"","sources":["/home/felidrick/Projects/VueProjects/learn1/src/App.vue"],"sourcesContent":["<template>\n<h1> {{counter}} </h1>\n<h1 v-once> {{counter}} </h1>\n<!-- <input type=\"text\" v-bind=\"{newName}\" @click=\"changeName()\"> -->\n<button @click=\"add(5)\">add 5</button>\n<button @click=\"remove(3)\">remove 3</button>\n<br>\n\n<p>full name : {{ fullName }}</p>\n<input type=\"text\" v-model=\"name\">\n<input type=\"text\" v-model=\"name\">\n<br>\n<button @click=\"resetName\">reset fullName</button>\n</template>\n\n<script>\n// import HelloWorld from './components/HelloWorld.vue'\n\nexport default {\n  // name: 'App',\n  // components: {\n  //   HelloWorld\n  // }\n  data(){\n    return {\n      counter:0,\n      name:'',\n      lastName:'',\n      // fullName:'',\n    }\n  },\n  methods:{\n    add(num){\n      this.counter += num;\n    },\n    remove(num){\n      this.counter -= num;\n    },\n    setName(){\n      this.name = event.target.value;\n    },\n    resetName(){\n      console.log('reset name executed');\n      this.name = '';\n      this.lastName = '';\n    },\n  },\n  watch:{\n    name(value){\n      if(value === ''){\n        return '';\n      }else{\n        return value\n      }\n    }\n  },\n  computed:{\n    fullName(){\n      console.log('full name executed');\n      if(this.name === '') {\n        return '';\n      }else{\n        return this.name + ' ' + 'Shayo';\n      }\n    }\n  }\n}\n\n</script>\n\n<style>\n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n  margin-top: 60px;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}